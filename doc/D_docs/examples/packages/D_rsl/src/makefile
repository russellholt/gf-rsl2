# $Id: makefile,v 2.3 1998/04/20 18:52:29 toddm Exp $INSTALL=/usr/sbin/installinclude $(MAKEFILE_LOCAL)INCLUDE =	-I. -I$(GRANITE)/granitecore/include -I$(LOCAL_INC) -I$(DCORE)CPPFLAGS = 	$(DBG_FLAG) -features=rtti $(FPIC) $(INCLUDE)LDDFLAGS = -L. -L$(RWDIR) $(RWLIB) -L/sw/opt/SUNWspro/SC4.2/lib -L$(DCORE)  -lDPACKAGE = D_rslLIBSONAME = lib$(PACKAGE).soLIBANAME = lib$(PACKAGE).astuff = Dlib_def.o######################################################################system: lib$(PACKAGE).so$(LIBSONAME): $(stuff)	$(CCC) $(LDDFLAGS) $(SHARE) $(stuff) -o $@	mv $(LIBSONAME) ../..clean:	$(RM) *.o *.a *.so	$(RM) -rf Templates.DB